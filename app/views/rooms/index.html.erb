<p id="notice"><%= notice %></p>

<h1>Rooms</h1>

<table>
  <thead>
    <tr>
      <th>Time</th>
      <% @rooms.each do |room| %>
          <th class="Room name"><%= room.name %></th>
      <% end %>
    </tr>
      <% i = 9.00 %>
      <% 17.times do |row| %>
        <tr>
          <% if i % 1 ==0 %>
            <td class="timeCell"><%= i %></td>
            <% @rooms.each do |room| %>
              <td class="roomCell" id="<%= room.name %>" data-num="<%= i %>">-</td>
            <% end %>
            <% i += 0.30 %>
          <% else %>
          <td class="timeCell"> <%= i %></td>
          <% @rooms.each do |room| %>
            <td class="roomCell" id="<%= room.name %>" data-num="<%= i %>">-</td>
          <% end %>
            <% i += 0.70 %>
        <% end %>
        </tr>
      <% end %>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>




  </tbody>
</table>

<br>
<% @rooms.each do |room| %>
    <td><%= link_to 'Show', room %></td>
<% end %>
<%= link_to 'New Room', new_room_path %>
